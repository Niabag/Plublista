{
  "name": "reels-automation",
  "version": "1.0.0",
  "description": "End-to-end pipeline to generate, edit, and auto-publish Instagram Reels from code snippets",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "node --watch server/index.js",
    "dev:client": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "stream:app": "electron electron-app/main.js",
    "stream:start": "concurrently \"npm run dev:server\" \"npm run stream:app\"",
    "setup:playwright": "npx playwright install chromium"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@fal-ai/client": "^1.9.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ffmpeg-static": "^5.3.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "playwright": "^1.50.0",
    "sqlite3": "^5.1.6",
    "ws": "^8.14.2",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^28.3.3",
    "lucide-react": "^0.294.0",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "tailwindcss": "^3.3.6",
    "vite": "^5.0.8"
  }
}
